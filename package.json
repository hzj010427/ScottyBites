    "sharp": "^0.34.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "uuid": "^10.0.0"
  },
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
