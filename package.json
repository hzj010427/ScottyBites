    "eslint": "^9.8.0",
    "events": "^3.3.0",
    "globals": "^15.9.0",
    "http-terminator": "^3.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "nock": "^14.0.3",
    "nodemon": "^3.1.4",
