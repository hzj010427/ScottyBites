    "util": "^0.12.5",
    "vite": "^6.2.3",
    "vm-browserify": "^1.1.2"
  },
  "targets": {
    "client": {
      "source": [
        "client/pages/index.html",
        "client/pages/auth.html",
        "client/pages/directory.html",
        "client/pages/verification.html",
        "client/pages/account.html",
        "client/pages/profile.html",
        "client/pages/businessCatalog.html",
        "client/pages/businessProfile.html",
        "client/pages/addBusiness.html",
        "client/pages/post.html",
        "client/pages/postFeed.html",
        "client/pages/createPost.html",
        "client/pages/map.html",
        "client/views/template.pug"
      ],
      "distDir": ".dist/client",
      "context": "browser"
    },
    "server": {
      "source": "server/serve.ts",
      "distDir": ".dist/server",
      "context": "node",
      "includeNodeModules": false
    }
  },
  "engines": {
    "node": "^20.16.0",
    "npm": ">=10.8.0"
  }
}
