{
  "name": "ScottyBites-18652",
  "author": "Hakan Erdogmus",
  "license": "None",
  "repository": {
    "type": "git",
    "url": "git+https://username:pat@github.com/cmu-fsd/repoName.git"
  },
  "scripts": {
    "clean:dist": "rm -rf ./.dist",
    "clean:all": "rm -rf ./.parcel-cache ./.dist",
    "build": "parcel build",
    "build:server": "tsc && ((cp -r .dist/client .server.debug/.dist) || (mkdir .server.debug/.dist &&  cp -r .dist/client .server.debug/.dist))",
    "build:trials": "tsc trials/ts-eg/*.ts --esModuleInterop --sourceMap --outDir trials/ts-eg/.out",
    "watch:trials": "tsc trials/ts-eg/*.ts --esModuleInterop --sourceMap --outDir trials/ts-eg/.out --watch",
    "serve:index": "parcel client/pages/index.html --dist-dir .client.debug --no-cache",
    "serve:auth": "parcel client/pages/auth.html --dist-dir .client.debug --no-cache",
    "serve:chat": "parcel client/pages/chat.html --dist-dir .client.debug --no-cache",
    "serve:friends": "parcel client/pages/friends.html --dist-dir .client.debug --no-cache",
    "clean:build": "npm-run-all clean:all build",
    "start": "node ./.dist/server/serve",
    "watch": "nodemon --watch client --watch server --watch .env -e js,ts,html,pug,css,json --exec npm-run-all build start",
    "watch:client": "parcel client/pages/**/*.html --dist-dir .client.debug --no-cache",
    "test:model": "jest ./tests/server.tests/model.tests --env=node",
    "test:unit": "jest tests/unit --coverage --coverageDirectory=.coverage/unit --env=node",
    "test:integration": "jest tests/api --coverage --coverageDirectory=.coverage/integration --env=node",
    "test:all": "npm run test:unit && npm run test:integration",
    "lint": "eslint ./",
    "postfixCalc": "ts-node ./trials/ts-eg/postfixCalc",
    "serve": "ts-node server/serve",
    "later": "echo \"Some future command...\" && exit 1",
    "dev": "vite"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-directions": "^4.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "compressorjs": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^12.6.2",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.10.0",
    "mongoose": "^8.5.2",
    "motion-dom": "^12.6.3",
